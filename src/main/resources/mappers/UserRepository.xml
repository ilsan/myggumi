<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ilsan.myggumi.web.repository.UserRepository">

    <select id="findByUserEmail" parameterType="String" resultType="MemberSession">
        select "userNo"
              ,"password"
              ,"email"
              ,"name"
              ,"address"
              ,"role"
        from USERS
        where "name" = #{username}
    </select>

</mapper>